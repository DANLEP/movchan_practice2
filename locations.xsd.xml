<?xml version="1.0" encoding="UTF-8"?>

<loc:locations
        xmlns:loc="http://it.nure.ua/xml/entity/location/"
        xmlns:plac="http://it.nure.ua/xml/entity/place/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://it.nure.ua/xml/entity/location/ locations.xsd ">
  <!--Zero or more repetitions:-->
  <loc:location id="3">
    <loc:title>string</loc:title>
    <loc:area>
      <!--3 or more repetitions:-->
      <loc:coordinate>
        <plac:Latitude>90.0000</plac:Latitude>
        <plac:Longitude>180.0000</plac:Longitude>
      </loc:coordinate>
      <loc:coordinate>
        <plac:Latitude>90.0000</plac:Latitude>
        <plac:Longitude>180.0000</plac:Longitude>
      </loc:coordinate>
      <loc:coordinate>
        <plac:Latitude>90.0000</plac:Latitude>
        <plac:Longitude>180.0000</plac:Longitude>
      </loc:coordinate>
    </loc:area>
    <loc:places>
      <!--1 or more repetitions:-->
      <loc:place id="3" isRecommended="false" rating="0">
        <plac:title>string</plac:title>
        <plac:description>stringstring</plac:description>
        <plac:type>Active</plac:type>
        <!--Optional:-->
        <plac:address>
          <plac:Street>string</plac:Street>
          <plac:HouseNumber>
            <!--You have a CHOICE of the next 2 items at this level-->
            <plac:Number>3</plac:Number>
          </plac:HouseNumber>
          <plac:City>string</plac:City>
          <plac:PostalCode>string</plac:PostalCode>
        </plac:address>
        <plac:coordinate>
          <plac:Latitude>90.0000</plac:Latitude>
          <plac:Longitude>180.0000</plac:Longitude>
        </plac:coordinate>
        <!--Optional:-->
        <plac:visitTime>12:00-13:20</plac:visitTime>
        <!--Optional:-->
        <plac:entranceFee>
          <plac:Price>1000.000</plac:Price>
          <plac:Currency>UAH</plac:Currency>
        </plac:entranceFee>
        <!--Optional:-->
        <plac:photos>
          <!--0 to 10 repetitions:-->
          <plac:photo>http://example.com/photo1.jpg</plac:photo>
        </plac:photos>
        <plac:seasonality>All Season</plac:seasonality>
        <!--Optional:-->
        <plac:tags>
          <!--1 or more repetitions:-->
          <plac:Tag>string</plac:Tag>
        </plac:tags>
      </loc:place>
    </loc:places>
  </loc:location>
</loc:locations>